<PhysiCell_settings version="1.4.1">
	<domain>
		<x_min>-500</x_min>
		<x_max>500</x_max>
		<y_min>-500</y_min>
		<y_max>500</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder>  

		<full_data>
			<interval units="min">6</interval>
			<enable>true</enable>
		</full_data>
		 
		<SVG>
			<interval units="min">6</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
        
        <tumor_radius type="double">150</tumor_radius>
        <enable_ecm_outputs type="bool">true</enable_ecm_outputs>
        <initial_ECM_density type="double">0.5</initial_ECM_density>
		<initial_anisotropy type="double">0.0</initial_anisotropy>
		<initial_leader_cell_fraction type="double">0.0</initial_leader_cell_fraction>
        <leader_adhesion type="double">6.5</leader_adhesion>
        <leader_repulsion type="double">25</leader_repulsion>
        <follower_adhesion type="double">6.5</follower_adhesion>
        <follower_repulsion type="double">25</follower_repulsion>
        <default_cell_speed type="double">0.3</default_cell_speed>
        <leader_motility_mode type="bool">true</leader_motility_mode>
        <follower_motility_mode type="bool">true</follower_motility_mode>
		<!-- <max_migration_speed type="double">0.3</max_migration_speed> -->
		<default_persistence_time type="double">10.0</default_persistence_time>
		<default_chemotaxis_bias type = "double">0.0</default_chemotaxis_bias>
		<default_ECM_sensitivity type = "double">1.0</default_ECM_sensitivity>
		<oxygen_migration_bias_for_leaders type = "double">0.95</oxygen_migration_bias_for_leaders>
		<ecm_uptake_rate type = "double">0.0</ecm_uptake_rate>
		
	</user_parameters>
	
</PhysiCell_settings>
